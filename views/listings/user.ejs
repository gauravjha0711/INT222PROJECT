<% layout("/layouts/boilerplate") %> 
<body>
    
    <div class="main-div">
        <h5>User Profile  </h5>
    <div class="card inner-div" style="width: 18rem;">
        <div class="card-body inner-div">
            <img src="data:image/jpeg;base64," style="height:150px" />
            <hr/>
            <p>Username:-  @<b><%= currUser.username %></b> </p>
            <p>Email:- <b><%= currUser.email %></b></p>
            <p>User id :- <b> <%= currUser._id %>  </b>  </p>
            <a href="/listings/info/edit/<%= currUser._id %>" class="btn btn-outline-danger" >Change Username</a>
            <a href="/listings/info/edit/password/<%= currUser._id %>" class="btn btn-outline-success"  >Change password</a>
        </div>
      </div>
    </div>
</body>